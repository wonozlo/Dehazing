{
    "exp_name": "translation_cyclegan_exp_0",
    "model_module": "cycle_gan",
    "model_name": "CycleGAN",
    "mode": "training",

    "data_loader_module": "image_dehazing",
    "data_loader_name": "DehazeLoader",
    "data_loader_workers": 8,

    "data_mode": "imgs",
    "data_path": "./data/dehaze",
    "data_exts": ["*.jpg"],
    "crop_size": 256,
    "train_batch_size": 4,
    "val_batch_size": 1,

    "cuda": true,
    "gpu_device": 1,
    "seed": 12345,
  
    "input_nc": 3,
    "output_nc": 3,
    "ngf": 64,
    "ndf": 64,
    "ng_blocks": 9,
    "nd_layers": 3,

    "lambda_idt": 10,
    "lambda_cycle": 20,
    "lambda_gan":  1,
    "lambda_l1": 10,
  
    "learning_rate": 0.0002,
    "beta1": 0.5,
    "beta2": 0.999,
    "gamma": 0.1,
    "schedule": [12000],
  
    "max_epoch": 400,
 
    "validate_every": 1,
    "save_every": 20
}